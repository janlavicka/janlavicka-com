{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "janlavicka-com",
  "main": "src/worker.ts",
  "compatibility_date": "2025-09-15",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "vars": {
    "VITE_APP_URL": "https://janlavicka.com"
  },
  "routes": [
    {
      "pattern": "janlavicka.com/*",
      "zone_name": "janlavicka.com"
    }
  ],
  "placement": {
    "mode": "smart"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "CLAP_COUNTER",
        "class_name": "ClapCounterDurableObject"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["ClapCounterDurableObject"]
    }
  ]
}
